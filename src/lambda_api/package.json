{
    "name": "lambda_api",
    "version": "0.2.0",
    "description": "Serverless Proxy",
    "author": "Geoicons",
    "license": "GPL-3.0",
    "scripts": {
        "build": "tsc",
        "test": "jest"
    },
    "jest": {
        "preset": "ts-jest",
        "roots": [
            "./"
        ],
        "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.[t]sx?$",
        "testEnvironment": "node",
        "testTimeout": 100000,
        "moduleFileExtensions": [
            "js",
            "jsx",
            "ts",
            "tsx"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "transformIgnorePatterns": [
            "/node_modules/(?!axios/.*)"
        ]
    },
    "dependencies": {
        "@types/fs-extra": "^9.0.13",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/jszip": "^3.4.1",
        "@types/jwk-to-pem": "^2.0.1",
        "@types/luxon": "^3.3.1",
        "@types/uuid": "^9.0.1",
        "@vendia/serverless-express": "^4.10.1",
        "ajv": "^8.12.0",
        "aws-jwt-verify": "^4.0.0",
        "aws-lambda": "^1.0.7",
        "axios": "^1.2.0",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "express": "^4.18.2",
        "fs-extra": "^11.1.0",
        "jest": "^29.7.0",
        "jose": "^4.14.1",
        "jsonwebtoken": "^9.0.0",
        "jszip": "^3.10.1",
        "jwk-to-pem": "^2.0.5",
        "luxon": "^3.3.0",
        "serverless-http": "^3.2.0",
        "stripe": "^12.8.0",
        "supertest": "^7.1.1",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.109",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.14"
    }
}